  r *= Math.floor(Math.random()*2) == 1 ? 1 : -1;
  image1.style.transform = "translate(" + x + "px, " + y + "px) rotate(" + r + "deg)"
    },
    500
  );
 setTimeout(
    () => {
    ActualRotation()
    },
    1000
  );
}
function pickColor(){
    r = Math.random() * (254 - 0) + 222;
    g = Math.random() * (254 - 0) + 222;
    b = Math.random() * (254 - 0) + 222;

    Color = 'rgb('+r+','+g+', '+b+')';
}

function DarkMode(){
